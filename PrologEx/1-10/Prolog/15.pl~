жить(зебра, земля).
жить(собака, земля).
жить(карп, вода).
жить(кит, вода).
жить(кошка, Х):-жить(собака,Х).
жить(крокодил, земля).
жить(крокодил, вода).
жить(лягушка, земля).
жить(лягушка, вода).
жить(утка, земля).
жить(утка, вода).
жить(утка, воздух).
жить(орел, земля).
жить(буревестник, вода).
жить(буревестник, воздух).
жить(орел, воздух).
жить2б(Х):-жить(Х, У), жить(Х, Г), У\=Г.
жить1(Х):-жить(Х, У), not(жить2б(Х)).
жить3(Х):-жить(Х, У), жить(Х, Ц), жить(Х, К), У\=Ц, Ц\=К, У\=К.
жить2т(Х):-жить2б(Х), not(жить3(х)).

